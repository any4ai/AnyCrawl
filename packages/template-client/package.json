{
    "name": "@anycrawl/template-client",
    "version": "1.0.0-beta.15",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "scripts": {
        "dev": "tsc --watch",
        "build": "tsc",
        "clean": "rm -rf dist",
        "test": "NODE_OPTIONS=--experimental-vm-modules jest --detectOpenHandles --forceExit",
        "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch --detectOpenHandles",
        "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage --detectOpenHandles --forceExit",
        "create:test:template": "dotenv-run -r ../.. -- tsx ./src/scripts/run-create-template.ts",
        "typecheck": "tsc --noEmit"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        },
        "./client": {
            "types": "./dist/client/index.d.ts",
            "import": "./dist/client/index.js",
            "default": "./dist/client/index.js"
        },
        "./sandbox": {
            "types": "./dist/sandbox/index.d.ts",
            "import": "./dist/sandbox/index.js",
            "default": "./dist/sandbox/index.js"
        },
        "./cache": {
            "types": "./dist/cache/index.d.ts",
            "import": "./dist/cache/index.js",
            "default": "./dist/cache/index.js"
        },
        "./types": {
            "types": "./dist/types/index.d.ts",
            "import": "./dist/types/index.js",
            "default": "./dist/types/index.js"
        }
    },
    "dependencies": {
        "@anycrawl/db": "workspace:*",
        "@anycrawl/libs": "workspace:*",
        "minimatch": "^10.0.3"
    },
    "devDependencies": {
        "@anycrawl/js-sdk": "^0.0.1",
        "@anycrawl/typescript-config": "workspace:*",
        "@jest/globals": "^29.7.0",
        "@types/jest": "^29.5.14",
        "@types/node": "^20.0.0",
        "@types/supertest": "^6.0.3",
        "dotenv": "^17.2.2",
        "jest": "^29.7.0",
        "supertest": "^7.1.4",
        "ts-jest": "^29.3.2",
        "typescript": "5.8.2"
    },
    "peerDependencies": {
        "@anycrawl/scrape": "workspace:*"
    },
    "peerDependenciesMeta": {
        "@anycrawl/scrape": {
            "optional": true
        }
    }
}
